        "@vue/cli-plugin-typescript": "^4.5.13",
        "@vue/cli-service": "~4.5.0",
        "@vue/compiler-sfc": "^3.0.0",
        "@vue/eslint-config-prettier": "^6.0.0",
        "@vue/eslint-config-typescript": "^5.0.2",
        "autoprefixer": "^10.4.13",
        "babel-eslint": "^10.1.0",
        "core-js": "^3.6.5",
