{
    "name": "vue3-draggable",
    "version": "0.0.1",
    "private": false,
    "description": "simple drag&drop component for vue 3.x",
    "author": {
        "name": "Jamie Lee",
        "email": "walker200665@gmail.com"
    },
    "scripts": {
        "serve": "vue-cli-service serve ./examples/main.ts --open --mode local",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint",
        "dev": "rollup -c -w",
        "build-library": "vue-cli-service build --target lib --name vue3-draggable ./src/index.js"
    },
    "main": "dist/vue3-draggable.umd.min.js",
    "files": [
        "dist/*.umd.min.*"
    ],
    "devDependencies": {
        "@rollup/plugin-commonjs": "^24.0.1",
        "@rollup/plugin-node-resolve": "^15.0.1",
        "@typescript-eslint/eslint-plugin": "^2.33.0",
        "@typescript-eslint/parser": "^2.33.0",
        "@vue/cli-plugin-babel": "~4.5.0",
        "@vue/cli-plugin-eslint": "~4.5.0",
        "@vue/cli-plugin-typescript": "^4.5.13",
        "@vue/cli-service": "~4.5.0",
        "@vue/compiler-sfc": "^3.0.0",
        "@vue/eslint-config-prettier": "^6.0.0",
        "@vue/eslint-config-typescript": "^5.0.2",
        "babel-eslint": "^10.1.0",
        "core-js": "^3.6.5",
        "eslint": "^6.7.2",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-vue": "^7.0.0-0",
        "prettier": "^1.19.1",
        "rollup": "^3.14.0",
        "rollup-plugin-typescript2": "^0.34.1",
        "rollup-plugin-vue": "^6.0.0",
        "typescript": "~4.1.5",
        "vue": "^3.0.0"
    },
    "peerDependencies": {
        "vue": "^3.0.0"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "@vue/prettier"
        ],
        "parserOptions": {
            "parser": "@typescript-eslint/parser"
        },
        "rules": {}
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead"
    ],
    "bugs": "https://github.com/shkilo/vue3-draggable/issues",
    "keywords": [
        "draggable",
        "vue3",
        "vue",
        "drag",
        "drop",
        "drag and drop"
    ],
    "license": "MIT",
    "repository": "https://github.com/shkilo/vue3-draggable"
}
